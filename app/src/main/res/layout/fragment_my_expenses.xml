<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".Fragments.MyExpenses">
<androidx.appcompat.widget.LinearLayoutCompat
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="match_parent">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="30dp"
                android:id="@+id/expenses_back_button"
                android:src="@drawable/arrow_back"
                android:layout_height="30dp">
            </androidx.appcompat.widget.AppCompatImageView>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:text="My Expenses"
                android:layout_marginLeft="5dp"
                android:fontFamily="@font/inter"
                android:letterSpacing="0.1"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_height="match_parent">
            </androidx.appcompat.widget.AppCompatTextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.appcompat.widget.Toolbar>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:text="Expenses"
            android:fontFamily="@font/inter"
            android:letterSpacing="0.1"
            android:layout_weight="1.5"
            android:gravity="center"
            android:textSize="14dp"
            android:textColor="@color/black"
            android:textStyle="normal"
            android:layout_height="wrap_content">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:text="Amount"
            android:fontFamily="@font/inter"
            android:letterSpacing="0.1"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="14dp"
            android:textColor="@color/black"
            android:textStyle="normal"
            android:layout_height="wrap_content">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:text="Approvals"
            android:fontFamily="@font/inter"
            android:letterSpacing="0.1"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="14dp"
            android:textColor="@color/black"
            android:textStyle="normal"
            android:layout_height="wrap_content">
        </androidx.appcompat.widget.AppCompatTextView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        app:cardElevation="5dp"
        app:cardCornerRadius="5dp"
        app:cardBackgroundColor="@color/white"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_weight="1.5"
                android:layout_height="wrap_content">
          <androidx.appcompat.widget.LinearLayoutCompat
              android:layout_width="wrap_content"
              android:orientation="vertical"
              android:layout_weight="1"
              android:gravity="center"
              android:layout_height="wrap_content">
              <androidx.appcompat.widget.AppCompatTextView
                  android:layout_width="wrap_content"
                  android:text="Clint meeting"
                  android:fontFamily="@font/inter"
                  android:letterSpacing="0.1"
                  android:gravity="center"
                  android:textSize="13dp"
                  android:textColor="@color/black"
                  android:textStyle="normal"
                  android:layout_height="wrap_content">
              </androidx.appcompat.widget.AppCompatTextView>
              <androidx.appcompat.widget.AppCompatTextView
                  android:layout_width="wrap_content"
                  android:text="20 sep 12:00 pm"
                  android:fontFamily="@font/inter"
                  android:letterSpacing="0.1"
                  android:gravity="center"
                  android:textSize="10dp"
                  android:layout_marginBottom="10dp"
                  android:textColor="@color/leave_bal"
                  android:textStyle="normal"
                  android:layout_height="wrap_content">
              </androidx.appcompat.widget.AppCompatTextView>
          </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="20dp"
                android:src="@drawable/viewdoc"
                android:layout_height="30dp">
            </androidx.appcompat.widget.AppCompatImageView>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="10dp"
                android:text="2"
                android:textStyle="bold"
                android:textSize="14dp"
                android:fontFamily="@font/rubik_bold"
                android:textColor="@color/profile_card_applyleave"
                android:layout_height="20dp">
            </androidx.appcompat.widget.AppCompatTextView>
            </androidx.appcompat.widget.LinearLayoutCompat>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:text="Rs 2000"
            android:fontFamily="@font/inter"
            android:letterSpacing="0.1"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="14dp"
            android:textColor="@color/black"
            android:textStyle="normal"
            android:layout_height="wrap_content">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:text="Pending"
            android:fontFamily="@font/inter"
            android:letterSpacing="0.1"
            android:layout_weight="2"
            android:gravity="center"
            android:textSize="14dp"
            android:textColor="@color/pending_col"
            android:textStyle="normal"
            android:layout_height="wrap_content">
        </androidx.appcompat.widget.AppCompatTextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        app:cardElevation="5dp"
        app:cardCornerRadius="5dp"
        app:cardBackgroundColor="@color/white"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_weight="1.5"
                android:layout_height="wrap_content">
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:text="Team Dinner"
                        android:fontFamily="@font/inter"
                        android:letterSpacing="0.1"
                        android:gravity="center"
                        android:textSize="13dp"
                        android:textColor="@color/black"
                        android:textStyle="normal"
                        android:layout_height="wrap_content">
                    </androidx.appcompat.widget.AppCompatTextView>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:text="20 sep 12:00 pm"
                        android:fontFamily="@font/inter"
                        android:letterSpacing="0.1"
                        android:gravity="center"
                        android:textSize="10dp"
                        android:layout_marginBottom="10dp"
                        android:textColor="@color/leave_bal"
                        android:textStyle="normal"
                        android:layout_height="wrap_content">
                    </androidx.appcompat.widget.AppCompatTextView>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="20dp"
                    android:src="@drawable/viewdoc"
                    android:layout_height="30dp">
                </androidx.appcompat.widget.AppCompatImageView>
                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="10dp"
                    android:text="1"
                    android:textStyle="bold"
                    android:textSize="14dp"
                    android:fontFamily="@font/rubik_bold"
                    android:textColor="@color/profile_card_applyleave"
                    android:layout_height="20dp">
                </androidx.appcompat.widget.AppCompatTextView>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:text="Rs 1000"
                android:fontFamily="@font/inter"
                android:letterSpacing="0.1"
                android:layout_weight="2"
                android:gravity="center"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:textStyle="normal"
                android:layout_height="wrap_content">
            </androidx.appcompat.widget.AppCompatTextView>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:text="Approved"
                android:fontFamily="@font/inter"
                android:letterSpacing="0.1"
                android:layout_weight="2"
                android:gravity="center"
                android:textSize="14dp"
                android:textColor="@color/approval_col"
                android:textStyle="normal"
                android:layout_height="wrap_content">
            </androidx.appcompat.widget.AppCompatTextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.cardview.widget.CardView>
</androidx.appcompat.widget.LinearLayoutCompat>
</androidx.appcompat.widget.LinearLayoutCompat>